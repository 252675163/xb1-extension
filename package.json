{
    "name": "vscode-code-outline",
    "displayName": "Code Outline",
    "description": "A code outline tree provider for VSCode",
    "version": "0.1.1",
    "publisher": "patrys",
    "homepage": "https://github.com/patrys/vscode-code-outline",
    "repository": {
        "type": "git",
        "url": "https://github.com/patrys/vscode-code-outline"
    },
    "icon": "resources/icon.png",
    "galleryBanner": {
        "color": "#292d3e",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:xb1"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "xb1.refresh",
                "title": "刷新xb1",
                "icon": {
                    "dark": "resources/dark/icon-refresh.svg",
                    "light": "resources/light/icon-refresh.svg"
                }
            },
            {
                "command": "xb1.revealCurrentSymbol",
                "title": "在xb1中打开"
            }
        ],
        "views": {
            "explorer": [
                {
                    "id": "xb1",
                    "name": "xb1组件"
                }
            ]
        },
        "menus": {
            "editor/context": [
                {
                    "command": "xb1.revealCurrentSymbol",
                    "group": "navigation"
                }
            ],
            "view/title": [
                {
                    "command": "xb1.refresh",
                    "when": "view == xb1",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}
