{
    "name": "xb1",
    "displayName": "xb1",
    "description": "",
    "version": "0.0.1",
    "publisher": "252675163",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "main": "./out/extension",
    "activationEvents": [
        "onView:xb1"
    ],
    "contributes": {
        "commands": [
            {
                "command": "xb1.refresh",
                "title": "刷新xb1",
                "icon": {
                    "dark": "resources/dark/icon-refresh.svg",
                    "light": "resources/light/icon-refresh.svg"
                }
            },
            {
                "command": "xb1.revealCurrentSymbol",
                "title": "在左侧的xb1中展示"
            }
        ],
        "views": {
            "explorer": [
                {
                    "id": "xb1",
                    "name": "xb1组件"
                }
            ]
        },
        "menus": {
            "editor/context": [
                {
                    "command": "xb1.revealCurrentSymbol",
                    "group": "navigation"
                }
            ],
            "view/title": [
                {
                    "command": "xb1.refresh",
                    "when": "view == xb1",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
