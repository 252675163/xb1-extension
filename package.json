{
    "name": "vscode-code-xb1",
    "displayName": "xb1",
    "description": "xb1 tree provider for VSCode",
    "version": "0.1.5",
    "publisher": "252675163",
    "homepage": "https://gitee.com/wszdw/vscode-extension-xb1",
    "repository": {
        "type": "git",
        "url": "https://gitee.com/wszdw/vscode-extension-xb1"
    },
    "icon": "resources/icon.png",
    "galleryBanner": {
        "color": "#292d3e",
        "theme": "dark"
    },
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:xb1"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "xb1.refresh",
                "title": "刷新xb1",
                "icon": {
                    "dark": "resources/dark/icon-refresh.svg",
                    "light": "resources/light/icon-refresh.svg"
                }
            }
        ],
        "views": {
            "explorer": [
                {
                    "id": "xb1",
                    "name": "xb1组件"
                }
            ]
        },
        "menus": {
            "view/title": [
                {
                    "command": "xb1.refresh",
                    "when": "view == xb1",
                    "group": "navigation"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "publish": "vsce publish",
        "test": "node ./node_modules/vscode/bin/test",
        "demo": "vsce package"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "iconv-lite": "^0.4.22",
        "opn": "^5.3.0"
    }
}
